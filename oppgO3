package Oblig_innlevering1;
import static javax.swing.JOptionPane.*;
import static java.lang.Integer.parseInt;



public class OppgaveO3_oblig1 {
	public static void main(String[] args) {
	
		String str = showInputDialog("Skriv inn tall: ");
		int n = parseInt(str);
		
		long fakul = 1;
	    for (int i = 1; i <= n; i++) {
	        fakul *=i;
	    }
	    System.out.print(n+"!="+n);
	    while(n>1) {
	    	n -=1;
	    	System.out.print("*"+n);
	    }
	    System.out.print("="+fakul);
	}
}

